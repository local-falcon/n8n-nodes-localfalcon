{
  "name": "@local-falcon/n8n-nodes-localfalcon",
  "version": "1.0.8",
  "description": "n8n community node for Local Falcon API - Local SEO rank tracking and analysis",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "local-falcon",
    "seo",
    "local-seo",
    "rank-tracking",
    "google-maps",
    "local-search"
  ],
  "license": "MIT",
  "homepage": "https://www.localfalcon.com",
  "author": {
    "name": "Pia Miranda",
    "email": "pia@localfalcon.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/local-falcon/n8n-nodes-localfalcon.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "prepublishOnly": "npm run build && npm run"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/LocalFalconApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/LocalFalcon/LocalFalcon.node.js"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^18.0.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "globals": "^16.5.0",
    "gulp": "^5.0.1",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "peerDependencies": {},
  "dependencies": {}
}
